{
    "path": "datasets/pen-based.arff",
    "resultsDir": "results/pen-based",
    "verbose": false,
    "plotClusters": true,
    "parameters": {
        "arffFile": {
            "stringConversion": "int",
            "floatNormalization": "min-max",
            "missingDataImputation": "median"
        },
        "DBSCAN": {
            "min_samples": 20,
            "eps": 0.33,
            "n_jobs": -1
        },
        "kMeans": {
            "n_clusters": 8,
            "init": "random",
            "n_init": 10,
            "max_iter": 500,
            "tol": 0.0004,
            "verbose": false
        },
        "bisectingKMeans": {
            "n_clusters": 10,
            "init": "random",
            "n_init": 10,
            "max_iter": 500,
            "tol": 0.0004,
            "verbose": false
        },
        "kmeansPP": {
            "n_clusters": 10,
            "init": "k-means++",
            "n_init": 10,
            "max_iter": 500,
            "tol": 0.0004,
            "verbose": false
        },
        "fcm": {
            "n_clusters": 6,
            "m": 3,
            "max_iter": 500,
            "tol": 0.0004,
            "verbose": false
        }
    }
}
